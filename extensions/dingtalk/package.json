{
  "name": "@openclaw/dingtalk",
  "version": "2026.2.23",
  "description": "OpenClaw DingTalk channel plugin via Stream Mode",
  "type": "module",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "axios": "^1.6.0",
    "dingtalk-stream": "^2.1.4",
    "fluent-ffmpeg": "^2.1.3"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.28",
    "openclaw": "workspace:*"
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
